startup_message off # default: on

autodetach on # default:on

shell zsh

encoding UTF-8

caption always "%{=b}%{b}%{C}[stream]%{-}  %+010=%-w%8L>%{uB .my}*%n %t%{-}%+w%-014<  %-012=%{b m} "

term screen-256color

# bind F9 to "move to previous window"
bindkey -k k9 prev
# bind F10 to "move to next window"
bindkey -k k; next

# bind key see
# http://web.mit.edu/gnu/doc/html/screen_toc.html#SEC82
