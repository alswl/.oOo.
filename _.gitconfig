[color]
    diff = auto 
	ui = true
[diff]
	external = git_diff_wrapper
[pager]
	diff = less -r
[user]
	name = 
	email = 
[alias]
	s = status
	c = commit -av
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	ll = log --stat --abbrev-commit
	st = status
	sta = stash
	pl = pull
	ps = push
	df = diff
	ci = commit
	co = checkout
	spl = svn rebase
	sps = svn dcommit
[merge]
	tool = vimdiff
[push]
	default = matching
	;default = simple
